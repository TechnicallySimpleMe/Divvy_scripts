// Run query to see results.
resources
| extend typeDisplayName=case(type =~ 'microsoft.web/serverfarms','App Service plan',type)
| where ((type in~ ('microsoft.cache/redisenterprise/databases','microsoft.cache/redis','microsoft.vmwarecloudsimple/virtualmachines','microsoft.vmwarecloudsimple/dedicatedcloudnodes','microsoft.resourcegraph/queries','microsoft.azurestackhci/clusters','microsoft.analysisservices/servers','microsoft.containerservice/containerservices','microsoft.containerinstance/containergroups','microsoft.compute/hostgroups','microsoft.compute/hostgroups/hosts','microsoft.compute/virtualmachines','microsoft.compute/virtualmachinescalesets','microsoft.containerregistry/registries/webhooks','microsoft.containerservice/managedclusters','microsoft.kubernetes/connectedclusters','microsoft.datacatalog/catalogs','microsoft.baremetal/crayservers','microsoft.securitydetonation/chambers','microsoft.devtestlab/labs/virtualmachines','microsoft.digitaltwins/digitaltwinsinstances','microsoft.documentdb/databaseaccounts','microsoft.eventhub/clusters','microsoft.scvmm/virtualmachines','microsoft.all/virtualmachines','microsoft.extendedlocation/customlocations','microsoft.hybridcompute/machines','microsoft.vmware/virtualmachines','microsoft.vmware/machines','microsoft.hybriddata/datamanagers','microsoft.azuredata/postgresinstances','microsoft.azuredata/sqlmanagedinstances','microsoft.azuredata/sqlserverinstances','microsoft.kusto/clusters','microsoft.kusto/clusters/databases','microsoft.dbformysql/flexibleservers','microsoft.dbforpostgresql/flexibleservers','microsoft.dbforpostgresql/servergroups','microsoft.dbforpostgresql/serversv2','microsoft.redhatopenshift/openshiftclusters','microsoft.servicebus/namespaces/disasterrecoveryconfigs','microsoft.servicefabric/clusters','microsoft.servicefabric/managedclusters','microsoft.streamanalytics/clusters','microsoft.resources/deploymentscripts','microsoft.sql/azuresql','microsoft.sql/servers/databases','microsoft.projectarcadia/workspaces/sqlcomputes','microsoft.sql/servers/elasticpools','microsoft.sql/instancepools','microsoft.sql/managedinstances/databases','microsoft.sql/managedinstances','microsoft.dbformariadb/servers','microsoft.dbformysql/servers','microsoft.dbforpostgresql/servers','microsoft.sql/servers','microsoft.sqlvirtualmachine/sqlvirtualmachines','microsoft.sql/virtualclusters')) or (isempty(type)))
| where (type !~ ('microsoft.elastic/monitors'))
| where (type !~ ('providers.test/statefulibizaengines'))
| where (type !~ ('microsoft.datacollaboration/workspaces'))
| where (type !~ ('microsoft.securitydetonation/chambers'))
| where (type !~ ('microsoft.azurestack/registrations'))
| where (type !~ ('microsoft.intelligentitdigitaltwin/digitaltwins'))
| where (type !~ ('microsoft.network/networkmanagers'))
| where (type !~ ('microsoft.recoveryservicesbvtd2/vaults'))
| where (type !~ ('microsoft.recoveryservicesbvtd/vaults'))
| where (type !~ ('microsoft.recoveryservicesintd/vaults'))
| where (type !~ ('microsoft.recoveryservicesintd2/vaults'))
| where (type !~ ('microsoft.hybridnetwork/devices'))
| where (type !~ ('microsoft.hybridnetwork/vendors'))
| where (type !~ ('microsoft.hybridnetwork/networkfunctions'))
| where (type !~ ('microsoft.mobilenetwork/accesspoints'))
| where (type !~ ('microsoft.mobilenetwork/enodebs'))
| where (type !~ ('microsoft.mobilenetwork/mobiledevices'))
| where (type !~ ('microsoft.mobilenetwork/mobilenetworks'))
| where (type !~ ('microsoft.mobilenetwork/mobilenetworksites'))
| where (type !~ ('microsoft.mobilenetwork/packetcores'))
| where (type !~ ('microsoft.mobilenetwork/simprofiles'))
| where (type !~ ('microsoft.connectedcache/cachenodes'))
| where not((type =~ ('microsoft.sql/servers/databases')) and ((kind in~ ('system','v2.0,system','v12.0,system','v12.0,user,datawarehouse,gen2,analytics'))))
| where not((type =~ ('microsoft.sql/servers')) and ((kind =~ ('v12.0,analytics'))))
| where (type in~ ('microsoft.cache/redis','microsoft.dbformysql/flexibleservers','microsoft.dbformysql/servers','microsoft.dbforpostgresql/servers','microsoft.containerinstance/containergroups','microsoft.compute/virtualmachinescalesets','microsoft.compute/virtualmachines','microsoft.sql/servers','microsoft.sql/servers/databases','microsoft.dbformariadb/servers','microsoft.documentdb/databaseaccounts'))
| project name,typeDisplayName,id,type,location,subscriptionId
| sort by (tolower(tostring(name))) asc
